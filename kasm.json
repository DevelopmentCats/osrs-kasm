{
  "friendly_name": "OSRS Runelite",
  "name": "developmentcats/osrs-runelite-kasm",
  "description": "Old School RuneScape Runelite client in a containerized Kasm workspace. Access the popular OSRS third-party client through your web browser with persistent configuration support.",
  "image_src": "osrs_runelite.png",
  "architecture": [
    "amd64",
    "arm64"
  ],
  "compatibility": [
    "1.13.x",
    "1.14.x",
    "1.15.x"
  ],
  "uncompressed_size_mb": 850,
  "categories": [
    "Gaming"
  ],
  "docker_registry": "https://index.docker.io/v1/",
  "image": "ghcr.io/developmentcats/osrs-runelite-kasm:latest",
  "run_config": {
    "hostname": "kasm",
    "environment": {
      "VNC_PW": "password"
    },
    "ports": {
      "6901": "6901"
    },
    "labels": {
      "com.kasm.image": "osrs-runelite-kasm"
    }
  },
  "exec_config": {
    "first_launch": {
      "user": "root",
      "privileged": false,
      "cmd": "bash -c '/dockerstartup/kasm_default_profile.sh /dockerstartup/vnc_startup.sh /dockerstartup/kasm_startup.sh'"
    },
    "go": {
      "user": "kasm-user",
      "privileged": false,
      "cmd": "bash -c '/dockerstartup/vnc_startup.sh /dockerstartup/kasm_startup.sh'"
    }
  },
  "notes": "OSRS Runelite client workspace. Mount /home/kasm-user/.runelite for persistent settings. Java-based client with full game functionality including plugins and customization options."
}